import wollok.game.*
import player.*
import tiles.*

program abc {

//	CONFIG	
	game.title("Bobby carrot")
	game.height(20)
	game.width(20)
	game.ground("ground.png")
	
	game.addVisualCharacter(player)
	game.addVisualIn(new Carrot(), game.at(5,5))
	game.addVisualIn(new Carrot(), game.at(2,7))
	game.addVisualIn(new Carrot(), game.at(8,7))
	game.addVisualIn(new Carrot(), game.at(5,4))
	game.addVisualIn(new Carrot(), game.at(1,9))
	
	game.start()	
}